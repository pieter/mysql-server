#
# Test some warnings
#
--disable-warnings
drop table if exists t1;
--enable-warnings

create table t1 (a int);
insert into t1 values (1);
insert into t1 values ("hej");
insert into t1 values ("hej"),("då");
set SQL_WARNINGS=1;
insert into t1 values ("hej");
insert into t1 values ("hej"),("då");
drop table t1;
set SQL_WARNINGS=0;

