source include/master-slave.inc;
connection master;
set SQL_LOG_BIN=0;
set timestamp=200006;
drop table if exists foo;
create table foo(t timestamp not null,a char(1));
insert into foo ( a) values ('F');
select unix_timestamp(t) from foo;
connection slave;
drop table if exists foo;
load table foo from master;
select unix_timestamp(t) from foo;
connection master;
drop table foo;
save_master_pos;
connection slave;
sync_with_master;
connection master;
set SQL_LOG_BIN=1;
CREATE TABLE t1 (
 a int not null
) TYPE=MyISAM MAX_ROWS=4000 CHECKSUM=1;

INSERT INTO t1 VALUES (1);
save_master_pos;
connection slave;
sync_with_master;
drop table t1;
load table t1 from master;
check table t1;
connection master;
drop table t1;
save_master_pos;
connection slave;
sync_with_master;
