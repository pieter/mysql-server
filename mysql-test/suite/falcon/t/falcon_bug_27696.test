--source include/have_falcon.inc
SET STORAGE_ENGINE = Falcon;
#
# Bug #27696: Tables with non-ASCII names can't be created
#
--echo *** Bug #27696 ***
SET NAMES UTF8;
--disable_warnings
DROP TABLE IF EXISTS ä;
DROP TABLE IF EXISTS ı;
--enable_warnings

CREATE TABLE ä (ä int);
CREATE TABLE ı (ı int);

# Final cleanup
DROP TABLE ä;
DROP TABLE ı;
