#
# Tests with the ujis character set
#
--disable_warnings
drop table if exists t1;
--enable_warnings

#
# Test problem with LEFT()
#

create table t1 (c text character set ujis);
insert into t1 values (0xa4a2),(0xa4a3);
select hex(left(c,1)) from t1 group by c;
drop table t1;
