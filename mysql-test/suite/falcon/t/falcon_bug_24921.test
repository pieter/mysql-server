SET @@storage_engine = Falcon;
#
# Bug #24921: LIKE searches fail if index on utf8 column
#
--echo *** Bug #24921 ***
--disable_warnings
DROP TABLE IF EXISTS t1;
--enable_warnings

CREATE TABLE t1 (a char(4) CHARACTER SET utf8 COLLATE utf8_turkish_ci, KEY (a));
INSERT INTO t1 VALUES ('abc'), ('abc '), ('abcd');
SELECT count(*) FROM t1 WHERE a like 'abc%';

ALTER TABLE t1 Engine MyISAM;
SELECT count(*) FROM t1 WHERE a like 'abc%';

# Final cleanup.
DROP TABLE t1;
