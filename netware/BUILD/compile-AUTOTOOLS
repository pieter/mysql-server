#! /bin/sh                   

# debug
#set -x

# stop on errors
set -e

for package in . ./innobase
do
	(cd $package
	rm -rf config.cache autom4te.cache
	aclocal
	autoheader
	libtoolize --force
	aclocal
	automake --add-missing --force-missing
	autoconf)
done

