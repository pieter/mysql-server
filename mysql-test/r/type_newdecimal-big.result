drop procedure if exists sp1;
create procedure sp1 () begin 
declare v1, v2, v3, v4 decimal(16,12); declare v5 int; 
set v1 = 1; set v2 = 2; set v3 = 1000000000000; set v4 = 2000000000000; set v5 = 0; 
