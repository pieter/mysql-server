**** On Master ****
DROP DATABASE IF EXISTS tpcb;
CREATE DATABASE tpcb;
USE tpcb;
SET BINLOG_FORMAT = 'ROW';
CREATE TABLE account (
id int,
bid int,
balance decimal(10,2),
filler blob,
PRIMARY KEY(id)
) ENGINE=Falcon;
